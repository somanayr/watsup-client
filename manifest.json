{
	"manifest_version": 2,

	"name": "WATSAP client",
	"description": "Implements the WATSAP login protocol",
	"version": "1.0",
	
	"permissions": [
		"tabs", "<all_urls>", "activeTab"
	],
	
	"browser_action": {
		"default_popup": "wait_popup.html",
		"default_title": "WATSUP"
	},
	
	"background": {
		"scripts": ["background.js"]
	}
}